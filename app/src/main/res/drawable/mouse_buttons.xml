<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <solid android:color="#777657" />
            <stroke android:width="2dp" android:color="#000170" />
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle">
            <solid android:color="#FFF00F" />
            <stroke android:width="0.5dp" android:color="#001100" />
        </shape>
    </item>
</selector>